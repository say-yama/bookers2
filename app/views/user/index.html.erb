<%= render"books/list",user:@user %>
<%= render"books/new",book:@book %>

<h2>Users</h2>

<% @users.each do |user| %>
 <div>
    <p><%= image_tag user.get_profile_image(100,100) %></p>
    <p><%= user.name %></p>
    <p> 
     <span>
       <%= link_to "Show",user_path(user.id) %>
     </span> 
    </p>
 </div>
 <% end %>