<div class "d-flex p-2 bd-highlight">
 <div class="d-inline-flex p-2 bd-highlight">
  <div class="col-md-3">
  <%= render"books/list",user:@user %>
  <%= render"books/new",book:@book %>
 </div>
 </div>
 <div class="d-inline-flex p-2 bd-highlight">
  <div class = "container">
   <h2>Users</h2>
    <table class ="table">
      <thead>
       <tr>
        <th>image</th>
        <th>name</th>
        <th></th>
       </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td>
            <p><%= image_tag user.get_profile_image(100,100) %></p>
          </td>
          <td>
            <p><%= user.name %></p>
          </td>
          <td>
            <p> 
             <span>
               <%= link_to "Show",user_path(user.id) %>
             </span> 
            </p>
          </td>
        </tr>
         <% end %>
      </tbody>  
    </table>
  </div>
 </div>
</div>